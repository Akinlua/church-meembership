{
  "name": "church-management-frontend",
  "version": "1.0.3",
  "homepage": ".",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-datepicker": "^7.0.0",
    "axios": "^1.3.4",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.3.9",
    "react": "^18.2.0",
    "react-datepicker": "^8.0.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.10.1",
    "react-to-print": "^3.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win --x64",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:all": "npm run build && electron-builder --win --mac"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "electron": "^33.2.0",
    "electron-builder": "^25.0.5",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.17"
  },
  "author": "Church Membership",
  "description": "Church Membership Management Application",
  "repository": {
    "type": "git",
    "url": "https://github.com/Akinlua/church-meembership.git"
  },
  "build": {
    "extends": null,
    "appId": "com.plugin.churchmembership",
    "productName": "ChurchMembership",
    "files": [
      "build/**/*",
      "main.js"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/favicon.ico",
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis",
      "icon": "build/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Church Membership",
      "deleteAppDataOnUninstall": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Akinlua",
        "repo": "church-meembership"
      }
    ]
  }
}